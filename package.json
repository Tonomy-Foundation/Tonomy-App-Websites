{
  "name": "tonomy-app-websites",
  "private": true,
  "version": "1.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "universal-link-association": "yarn tsx universalLinkAssociation.ts",
    "build": "yarn run universal-link-association && tsc && NODE_OPTIONS=--max-old-space-size=6144 vite build",
    "preview": "vite preview",
    "lint:ts": "eslint src",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint:ts && yarn run lint:styles",
    "lint:fix": "yarn lint:ts --fix && yarn lint:styles --fix",
    "lint:pretty": "npx prettier --check .",
    "lint:pretty:fix": "npx prettier --write .",
    "typeCheck": "tsc --noEmit",
    "start:prod:accounts": "serve -s dist -l 3000 -n",
    "start:prod:demo": "serve -s dist -l 3001 -n",
    "start:prod:console": "serve -s dist -l 3002 -n",
    "updateSdkVersion": "./update_sdk_version.sh",
    "release": "semantic-release --debug"
  },
  "dependencies": {
    "@emotion/react": "^11.12.0",
    "@emotion/styled": "^11.12.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "^5.0.0-alpha.151",
    "@mui/material": "^5.16.4",
    "@reown/appkit": "^1.7.19",
    "@reown/appkit-adapter-wagmi": "^1.7.19",
    "@tanstack/react-query": "^5.85.3",
    "@tonomy/tonomy-id-sdk": "0.37.0",
    "@wharfkit/antelope": "^1.0.8",
    "rc-highlight": "^2.5.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-prism": "^4.3.2",
    "react-pro-sidebar": "^1.0.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.25.1",
    "react-toastify": "^11.0.5",
    "sql.js": "^1.10.3",
    "styled-components": "^6.1.12",
    "swiper": "^11.2.5",
    "viem": "^2.33.3",
    "wagmi": "^2.17.0",
    "web-vitals": "^4.2.2",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.14.11",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-lottie": "^1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "react-lottie": "^1.2.10",
    "semantic-release": "^24.2.0",
    "serve": "^14.2.0",
    "stylelint": "^16.7.0",
    "tsx": "^4.19.3",
    "typescript": "*",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.3.4"
  },
  "resolutions": {
    "jsonld": "link:./node_modules/@digitalcredentials/jsonld",
    "@wharfkit/antelope": "1.0.10",
    "ethers": "6.13.2"
  },
  "packageManager": "yarn@4.3.1",
  "engines": {
    "node": "^20.0.0",
    "yarn": "^4.3.1"
  }
}
